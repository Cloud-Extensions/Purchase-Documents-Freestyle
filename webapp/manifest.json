{
	"_version": "1.12.0",
	"sap.app": {
		"id": "fiori.create.purchase.documents.PurchaseDocumentsFreestyle",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"Z_UI_PURCHASEDOCUMENT_V2": {
				"uri": "/sap/opu/odata/sap/Z_UI_PURCHASEDOCUMENT_V2/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"annotations": ["Z_UI_PURCHASEDOCUMENT_V2_VAN"]
				}
			},
			"Z_UI_PURCHASEDOCUMENT_V2_VAN": {
				"uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='Z_UI_PURCHASEDOCUMENT_V2_VAN',Version='0001')/$value/",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": ""
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "fiori.create.purchase.documents.PurchaseDocumentsFreestyle.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "fiori.create.purchase.documents.PurchaseDocumentsFreestyle.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z_UI_PURCHASEDOCUMENT_V2",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "fiori.create.purchase.documents.PurchaseDocumentsFreestyle.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"transition": "show"
			},
			"routes": [
				{
					"name": "CreatePurchaseDocument",
					"pattern": "",
					"target": "CreatePurchaseDocument"
				},
				{
					"name": "DisplayPurchaseDocuments",
					"pattern": "DisplayPurchaseDocuments",
					"target": "DisplayPurchaseDocuments"
				}
			],
			"targets": {
				"CreatePurchaseDocument": {
					"viewName": "CreatePurchaseDocument"
				},
				"DisplayPurchaseDocuments":{
					"viewName":"DisplayPurchaseDocuments"	
				}
			}
		}
	}
}